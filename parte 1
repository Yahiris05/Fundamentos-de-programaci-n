class Pila:
    def __init__(self):
        self.items = []

    def apilar(self, item):
        self.items.append(item)

    def desapilar(self):
        if not self.esta_vacia():
            return self.items.pop()
        return None

    def esta_vacia(self):
        return len(self.items) == 0

    def cima(self):
        if not self.esta_vacia():
            return self.items[-1]
        return None


class ColaVehiculos:
    def __init__(self):
        self.pila_in = Pila()
        self.pila_out = Pila()

    def llegar_vehiculo(self, placa, tipo):
        vehiculo = {'placa': placa, 'tipo': tipo}
        self.pila_in.apilar(vehiculo)

    def estacionar_vehiculo(self):
        if self.pila_out.esta_vacia():
            while not self.pila_in.esta_vacia():
                self.pila_out.apilar(self.pila_in.desapilar())
        return self.pila_out.desapilar()

    def esta_vacia(self):
        return self.pila_in.esta_vacia() and self.pila_out.esta_vacia()


class NodoEstacionamiento:
    def __init__(self, placa, tipo):
        self.placa = placa
        self.tipo = tipo
        self.siguiente = None


class ZonaEstacionamiento:
    def __init__(self, nombre_zona):
        self.nombre_zona = nombre_zona
        self.cabeza = None
        self.final = None

    def ocupar_espacio(self, placa, tipo):
        nuevo_vehiculo = NodoEstacionamiento(placa, tipo)
        if not self.cabeza:
            self.cabeza = nuevo_vehiculo
            self.final = nuevo_vehiculo
        else:
            self.final.siguiente = nuevo_vehiculo
            self.final = nuevo_vehiculo

    def mostrar_zona(self):
        actual = self.cabeza
        print(f"Zona de Estacionamiento: {self.nombre_zona}")
        while actual:
            print(f"Vehículo Placa: {actual.placa}, Tipo: {actual.tipo}", end=" -> ")
            actual = actual.siguiente
        print("Libre")


# Ejemplo de uso

# Crear la cola de vehículos que llegan al estacionamiento
cola_vehiculos = ColaVehiculos()
cola_vehiculos.llegar_vehiculo("ABC123", "Sedan")
cola_vehiculos.llegar_vehiculo("DEF456", "SUV")
cola_vehiculos.llegar_vehiculo("GHI789", "Motocicleta")

# Crear la zona de estacionamiento para los clientes
zona_clientes = ZonaEstacionamiento("Clientes")

# Estacionar vehículos en la zona de clientes según el orden de llegada
while not cola_vehiculos.esta_vacia():
    vehiculo = cola_vehiculos.estacionar_vehiculo()
    zona_clientes.ocupar_espacio(vehiculo['placa'], vehiculo['tipo'])

# Mostrar el estado de ocupación de la zona de clientes
zona_clientes.mostrar_zona()
